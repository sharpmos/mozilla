{

  "manifest_version": 2,
  "name": "earnesty",
  "version": "1.0",

  "description": "Adds a blue border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["earnest.js"]
    }
  ]

}
